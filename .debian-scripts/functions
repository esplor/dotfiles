_dots () {
  printf '%*s\n' "${COLUMNS:-$(tput cols)}" '' | tr ' ' -
}

_get_stdout () {
    URL=$1

    if [ -x "$(which wget)" ] ; then
        curl -o- -sfL $URL
    elif [ -x "$(which curl)" ]; then
        wget -q $URL -O-
    else
        echo "Could not find curl or wget, please install one." >&2
    fi
}
