bar {
  position top
  height 32
  # gaps 5
  font Fira Code, pango:B612, Terminus, TerminessTTF Nerd Font, Material Icons, Font Awesome 6 Free, Font Awesome 6 Brands
  strip_workspace_numbers yes


  colors {
    #Sepia
    background #401e1b
    statusline #ece4da
    separator  #69848c
    focused_background #465459
    focused_statusline #f5eeef
    focused_separator  #657fbf

    #                  border  bckgrnd text
    focused_workspace  #f2a81d #465459 #f2a81d
    active_workspace   #657fbf #465459 #657fbf
    inactive_workspace #8abeb7 #465459 #8abeb7
    urgent_workspace   #cc4539 #465459 #cc4539
  }
}
